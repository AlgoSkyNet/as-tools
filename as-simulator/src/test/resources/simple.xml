<simulation>
	<space name="people" size="10" distributionRole="seeder">
		<fields>
			<sequence name="id" />
			<firstName />
			<lastName />
			<birthDate name="dob" />
			<address name="address" />
			<city name="city" />
			<regex name="phone" regex="[2-9][0-9]{2}-[0-9]{3}-[0-9]{4}" />
			<emailAddress name="email" />
		</fields>
	</space>
</simulation>