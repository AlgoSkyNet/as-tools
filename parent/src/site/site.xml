<?xml version="1.0" encoding="ISO-8859-1"?>
<project name="ActiveSpaces" xmlns="http://maven.apache.org/DECORATION/1.3.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/DECORATION/1.3.0 http://maven.apache.org/xsd/decoration-1.3.0.xsd">

	<bannerLeft>
		<name>ActiveSpaces</name>
		<href>index.html</href>
	</bannerLeft>

	<publishDate position="bottom" format="yyyy-MM-dd" />
	<version position="bottom" />

	<skin>
		<groupId>lt.velykis.maven.skins</groupId>
		<artifactId>reflow-maven-skin</artifactId>
		<version>1.1.0</version>
	</skin>

	<custom>
		<reflowSkin>
			<protocolRelativeURLs>true</protocolRelativeURLs>
			<smoothScroll>true</smoothScroll>
			<theme>bootswatch-spacelab</theme>
			<highlightJs>true</highlightJs>
			<highlightJsTheme>github</highlightJsTheme>
			<absoluteResourceURL>${project.url}</absoluteResourceURL>
			<brand>
				<name>TIBCO ActiveSpaces</name>
				<href>index.html</href>
			</brand>
			<slogan>A high-performance, distributed, persistent, and
				event-enabled data-grid</slogan>
			<topNav>ActiveSpaces|Tools|Community</topNav>
			<bottomNav maxSpan="9">
				<column>ActiveSpaces</column>
				<column>Tools</column>
				<column>Community</column>
			</bottomNav>
			<pages>
				<index>
					<titleTemplate>ActiveSpaces</titleTemplate>
					<breadcrumbs>false</breadcrumbs>
					<toc>false</toc>
					<markPageHeader>false</markPageHeader>
					<sections>
						<body />
					</sections>
				</index>
			</pages>
		</reflowSkin>
	</custom>

	<body>
		<head>
			<link rel="icon" type="image/png" href="favicon.ico" />
		</head>

		<menu name="ActiveSpaces">
			<item name="Enterprise Edition"
				href="http://www.tibco.com/products/automation/in-memory-computing/in-memory-data-grid/activespaces-enterprise-edition/default.jsp" />
			<item name="TIBCO Access Point"
				href="http://tap.tibco.com/storefront/trialware/tibco-activespaces/prod15306.html" />
			<item name="JDBC Driver"
				href="jdbcdriver" />
		</menu>

		<menu name="Tools">
			<item name="SpaceInvader" href="tools/spaceinvader" />
			<item name="File2AS" href="tools/file2as" />
			<item name="Excel2AS" href="tools/excel2as" />
			<item name="Simulator" href="tools/simulator" />
		</menu>

		<menu name="Community">
			<item name="Confluence"
				href="http://confluence.tibco.com/display/ftg/ActiveSpaces" />
			<item name="tibbr" href="https://tibco.tibbr.com/tibbr/#!/subjects/7305" />
		</menu>
	</body>
</project>